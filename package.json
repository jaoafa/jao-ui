{
  "name": "@jaoafa/jao-ui",
  "version": "0.1.4",
  "description": "jao UI Components with Vue.js.",
  "keywords": [
    "jaoafa",
    "vue"
  ],
  "homepage": "https://github.com/jaoafa/jao-ui",
  "bugs": "https://github.com/jaoafa/jao-ui/issues",
  "license": "MIT",
  "author": {
    "name": "Hiratake",
    "url": "https://hiratake.xyz"
  },
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "main": "dist/jao-ui.ssr.js",
  "browser": "dist/jao-ui.esm.js",
  "module": "dist/jao-ui.esm.js",
  "unpkg": "dist/jao-ui.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaoafa/jao-ui.git"
  },
  "sideEffects": [
    "*.vue"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.3.12",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vue/babel-preset-app": "^4.5.15",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.21",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-loader": "^8.2.3",
    "can-npm-publish": "^1.3.6",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "minimist": "^1.2.5",
    "postcss": "^8.3.11",
    "postcss-html": "^1.2.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.43.4",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "ts-jest": "^27.0.7",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.4",
    "vue": "^3.2.21",
    "vue-loader": "^16.8.2"
  },
  "peerDependencies": {
    "vue": "^3.2.21"
  },
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vue-cli-service serve dev/serve.ts --port 3000",
    "test": "jest --config=\".jestrc.js\"",
    "lint": "npm run lint:script & npm run lint:style",
    "lint:script": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:style": "stylelint \"**/*.{css,scss,vue}\"",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,vue,json}\" --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
    "prepublishOnly": "npm run build"
  }
}
