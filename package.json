{
  "name": "@jaoafa/jao-ui",
  "version": "0.0.14",
  "description": "jao UI Components with Vue.js.",
  "keywords": [
    "jaoafa",
    "vue"
  ],
  "homepage": "https://github.com/jaoafa/jao-ui",
  "bugs": "https://github.com/jaoafa/jao-ui/issues",
  "license": "MIT",
  "author": {
    "name": "Hiratake",
    "url": "https://hiratake.xyz"
  },
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "main": "dist/jao-ui.ssr.js",
  "browser": "dist/jao-ui.esm.js",
  "module": "dist/jao-ui.esm.js",
  "unpkg": "dist/jao-ui.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaoafa/jao-ui.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/jaoafa"
  },
  "sideEffects": [
    "*.vue"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^2.4.2",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue": "^6.3.8",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-loader": "^8.2.2",
    "can-npm-publish": "^1.3.6",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "minimist": "^1.2.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.41.1",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^22.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "serve": "cross-env PORT=3000 vue-cli-service serve dev/serve.ts",
    "lint": "npm run lint:js & npm run lint:style",
    "lint:js": "eslint . --ext .js,.ts,.vue",
    "lint:style": "stylelint \"**/*.{css,scss,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,json}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
    "prepublishOnly": "npm run build"
  }
}
