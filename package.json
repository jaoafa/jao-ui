{
  "name": "@jaoafa/jao-ui",
  "version": "0.0.11",
  "description": "jao UI Components with Vue.js.",
  "keywords": [
    "jaoafa",
    "vue"
  ],
  "homepage": "https://github.com/jaoafa/jao-ui",
  "bugs": "https://github.com/jaoafa/jao-ui/issues",
  "license": "MIT",
  "author": {
    "name": "Hiratake",
    "url": "https://hiratake.xyz"
  },
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "main": "dist/jao-ui.ssr.js",
  "browser": "dist/jao-ui.esm.js",
  "module": "dist/jao-ui.esm.js",
  "unpkg": "dist/jao-ui.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaoafa/jao-ui.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/jaoafa"
  },
  "sideEffects": false,
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "can-npm-publish": "^1.3.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.14.0",
    "husky": "^7.0.1",
    "minimist": "^1.2.5",
    "rollup": "^2.53.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.35.2",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^22.0.0",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "serve": "cross-env PORT=3000 vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "lint": "npm run lint:js & npm run lint:style",
    "lint:js": "eslint . --ext .js,.vue --fix",
    "lint:style": "stylelint \"**/*.vue\" --fix"
  }
}
